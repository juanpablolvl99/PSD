<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1"
  metadata-complete="true">

    	<description>
    
	</description>
    
	<display-name></display-name>
	
	<welcome-file-list>
		<welcome-file>index.htm</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>LoginUsuario</servlet-name>
	    <servlet-class>projeto.psd.servlets.LoginUsuario</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginUsuario</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CadastroUsuario</servlet-name>
		<servlet-class>projeto.psd.servlets.CadastroUsuario</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CadastroUsuario</servlet-name>
		<url-pattern>/cadastro</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Buscador</servlet-name>
		<servlet-class>projeto.psd.servlets.Buscador</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Buscador</servlet-name>
		<url-pattern>/buscador</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AdicionarFoto</servlet-name>
		<servlet-class>projeto.psd.servlets.AdicionaFoto</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdicionarFoto</servlet-name>
		<url-pattern>/AdicionarFoto</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EncerrarSessao</servlet-name>
		<servlet-class>projeto.psd.servlets.EncerrarSessao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EncerrarSessao</servlet-name>
		<url-pattern>/EncerrarSessao</url-pattern>
	</servlet-mapping>
</web-app>
